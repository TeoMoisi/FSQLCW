create database DB

use database DB

// create table Table1{{col1:FLOAT},{col2: STRING}}

create table Table1{col1:INT;name:STRING}

Table1.where(col1 lt 6).delete()

Table1.where(col1 eq 1).delete()

Table1.delete()

Table1.update({col1:5},{name:'teo'});

Table1.where(col1 gt 8).update({col1:7});

Table1.addColumn(col3:BOOL)

Table1.addColumns({col4:FLOAT},{(col3,col4) as primary key },{col5:INT})

Table1.dropColumn(col3)
Table1.dropColumns(col4,col3)

Table1.modify({col4:BOOL},{col5:STRING})

create table Table2{id:INT;name:STRING}


Table1.innerJoin(Table2).on(Table1.name = Table2.name).where(col1 eq 1).select(name, col1)
Table1.leftJoin(Table2).on(Table1.name = Table2.name).select(name,col1)


Table1.where(col1 ne 0).select(col1, name)